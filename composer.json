{
  "name": "originphp/app",
  "description": "OriginPHP Application",
  "homepage": "https://www.originphp.com",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.3.0",
    "originphp/framework": "^3.12",
    "originphp/commands": "^3.0",
    "originphp/cache": "^2.0",
    "originphp/log": "^2.0",
    "originphp/storage": "^3.0",
    "originphp/collection": "^2.0",
    "originphp/filesystem": "^3.0",
    "originphp/text": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "originphp/debug-plugin": "^2.0",
    "originphp/generate": "^3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php bin/install"
    ]
  }
}